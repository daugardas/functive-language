phoonk void foobar (int x) {
    if (x % 3 == 0 && x % 5 == 0)
        print "FooBar";
    else if (x % 5 == 0)
        print "Foo";
    else if (x % 3 == 0)
        print "Bar";
    else
        print "NOTFOOBAR";
}

phoonk int power(int base, int exponent) {
    int result = base;
    for (int i = 1; i < exponent; i++)
    {
        result = result * result;
    }

    return result;
}

foobar(97);

